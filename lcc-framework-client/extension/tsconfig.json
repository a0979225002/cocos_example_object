		"module":"commonjs",
		"lib": [ "dom", "es5", "es2015.promise" ],
        "target": "ES5",
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"noImplicitAny": false,
        "isolatedModules": false,
        "removeComments": false,
		"declaration": true,
		"allowJs": true,
		"types": [
			"../@types/editor",
			"node"
		],

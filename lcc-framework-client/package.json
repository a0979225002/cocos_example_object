    "gulp-notify": "^3.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "merge2": "^1.4.1",
    "through2": "^4.0.2",

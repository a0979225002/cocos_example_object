  "description": "LCC框架插件",
  "author": "Lujun",
  "main": "./bin/main.js",
  "main-menu": {
    "i18n:MAIN_MENU.package.title/i18n:lcc-framework.menu-extension/i18n:lcc-framework.menu-import-framework": {
      "message": "lcc-framework:import_framework"
